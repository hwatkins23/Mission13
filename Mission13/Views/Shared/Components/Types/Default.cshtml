@{
}

@model IEnumerable<string>

<a class="btn btn-sm btn-secondary navbar-btn" asp-controller="Home" asp-action="Index" asp-route-TeamName="">Home</a>
<br />
@foreach (string t in Model)
{
        <a class="btn btn-sm btn-secondary navbar-btn
           @(ViewBag.SelectedType == t ? "btn-primary" : "bg-dark")" asp-controller="Home" asp-action="Index" asp-route-TeamName="@t">@t</a>
        <br />
 }
    